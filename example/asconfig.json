{
  "targets": {
    "debug": {
      "outFile": "build/output.wasm",
      "textFile": "build/output.wat",
      "sourceMap": "output.wasm.map",
      "shrinkLevel": 0,
      "debug": true
    },
    "release": {
      "outFile": "build/output.wasm",
      "sourceMap": false,
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": false,
      "noAssert": false,
      "debug": false
    }
  },
  "options": {
    "bindings": "raw"
  }
}